<div class="container">
  <h3 class="text-center">contact form</h1>
  
  <%= form_for "/contacts" do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :commentaires %>
    <%= f.text_area :commentaires %>
    <%= f.submit 'valider' %>
  <% end %>

</div>
